[sandvine-svtcpa]
name=Sandvine - SVTCPA (Sandvine TCP Accelerator)
{% if (release == 'dev' and base_os == 'centos6') %}
baseurl=ftp://{{ sandvine_yum_url }}/images/Linux/CENTOS6-x64/TCPA/svtcpa-LNX-{{ svtcpa_version }}
{% endif %}
{% if (release == 'dev' and base_os == 'centos7') %}
baseurl=ftp://{{ sandvine_yum_url }}/images/Linux/RHEL7-x64/TCPA/svtcpa-LNX-{{ svtcpa_version }}
{% endif %}
enabled=1
protect=0
gpgcheck=0
sslverify=0
